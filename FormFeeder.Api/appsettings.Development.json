{
  "ConnectionStrings": {
    "DefaultConnection": "User ID=postgres;Password=postgres;Host=localhost;Port=5432;Database=formfeeder_dev;Include Error Detail=true"
  },
  "FormConfigurationProvider": {
    "Type": "AppSettings",
    "SeedFromAppSettings": true,
    "OverwriteExistingOnSeed": false
  },
  "Forms": [
    {
      "FormId": "contact-form",
      "AllowedDomains": ["*"],
      "Description": "Test contact form",
      "Enabled": true,
      "RateLimit": {
        "RequestsPerWindow": 100,
        "WindowMinutes": 1
      },
      "Connectors": [
        {
          "Type": "MailJet",
          "Name": "ContactFormNotification",
          "Enabled": true,
          "Settings": {
            "ApiKey": "MAILJET_APIKEY",
            "ApiSecret": "MAILJET_APISECRET",
            "FromEmail": "no-reply@example.com",
            "FromName": "FormFeeder OSS",
            "ToEmail": "test@example.com",
            "ToName": "Admin",
            "Subject": "New Contact Form Submission",
            "TemplateId": "123456"
          }
        },
        {
          "Type": "Slack",
          "Name": "SlackNotification",
          "Enabled": true,
          "Settings": {
            "WebhookUrl": "https://hooks.slack.com/services/SLACK_INBOUND_WEBHOOK",
            "Username": "FormFeeder",
            "IconEmoji": ":email:",
            "IncludeRawJson": "true"
          }
        }
      ]
    },
    {
      "FormId": "test-form",
      "AllowedDomains": ["*"],
      "Description": "Generic test form",
      "Enabled": true,
      "RateLimit": {
        "RequestsPerWindow": 100,
        "WindowMinutes": 1
      }
    },
    {
      "FormId": "privacy-form",
      "AllowedDomains": ["*"],
      "Description": "Privacy mode test form - no DB persistence",
      "Enabled": true,
      "PrivacyMode": true,
      "RateLimit": {
        "RequestsPerWindow": 50,
        "WindowMinutes": 1
      },
      "Connectors": [
        {
          "Type": "Slack",
          "Name": "PrivacyFormNotification",
          "Enabled": true,
          "Settings": {
            "WebhookUrl": "https://hooks.slack.com/services/SLACK_INBOUND_WEBHOOK",
            "Username": "Privacy Form Bot",
            "IconEmoji": ":lock:",
            "IncludeRawJson": "false"
          }
        }
      ]
    },
    {
      "FormId": "invalid-privacy-form",
      "AllowedDomains": ["*"],
      "Description": "Invalid privacy mode - no connectors",
      "Enabled": true,
      "PrivacyMode": true,
      "RateLimit": {
        "RequestsPerWindow": 50,
        "WindowMinutes": 1
      },
      "Connectors": []
    },
    {
      "FormId": "disabled-form",
      "AllowedDomains": ["*"],
      "Description": "This form is disabled",
      "Enabled": false,
      "RateLimit": {
        "RequestsPerWindow": 10,
        "WindowMinutes": 1
      }
    }
  ],
  "MailJetDefaults": {
    "ApiKey": "MAILJET_APIKEY",
    "ApiSecret": "MAILJET_APISECRET",
    "FromEmail": "no-reply@example.com",
    "FromName": "FormFeeder OSS",
    "ToEmail": "test@example.com",
    "ToName": "Admin",
    "Subject": "New Contact Form Submission",
    "TemplateId": "123456"
  },
  "RateLimiting": {
    "PerForm": {
      "DefaultPermitLimit": 100,
      "DefaultWindowMinutes": 1
    }
  },
  "AllowedOrigins": [
    "https://localhost:5501"
  ],
  "EnableSwagger": true,
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  }
}