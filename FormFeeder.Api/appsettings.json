{
  "ConnectionStrings": {
    "DefaultConnection": "Database=PROD-DB;Server=PROD-SERVER;User Id=PROD-USER;Password=PROD-PASS"
  },
  "FormConfigurationProvider": {
    "Type": "Database",
    "SeedFromAppSettings": true,
    "OverwriteExistingOnSeed": false
  },
  "RateLimiting": {
    "Global": {
      "PermitLimit": 1000,
      "WindowMinutes": 1,
      "QueueLimit": 10
    },
    "PerForm": {
      "DefaultPermitLimit": 10,
      "DefaultWindowMinutes": 1
    }
  },
  "RetryPolicy": {
    "MaxRetryAttempts": 3,
    "BaseDelaySeconds": 1,
    "MaxDelaySeconds": 30,
    "UseJitter": true,
    "JitterType": "Decorrelated",
    "BackoffType": "Exponential"
  },
  "AllowedOrigins": [
    "https://formfeeder.io",
    "https://www.formfeeder.io"
  ],
  "EnableSwagger": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}